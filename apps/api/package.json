{
  "name": "@irysbase/api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:enhanced": "ts-node-dev --respawn --transpile-only src/index-enhanced.ts",
    "build": "tsc",
    "build:enhanced": "tsc && cp src/schema-enhanced.graphql dist/",
    "start": "node dist/index.js",
    "start:enhanced": "node dist/index-enhanced.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@apollo/server": "^4.9.3",
    "@graphql-tools/schema": "^10.0.0",
    "@irys/query": "^0.0.13",
    "@irys/upload": "^0.0.15",
    "@irysbase/shared": "workspace:*",
    "@prisma/client": "^5.6.0",
    "bull": "^4.12.0",
    "bull-board": "^2.1.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "ethers": "^6.7.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prisma": "^5.6.0",
    "redis": "^4.6.8",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.49.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}